{
  "name": "remixer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config src/config/vite.config.ts",
    "build": "tsc && vite build --config src/config/vite.config.ts",
    "preview": "vite preview --config src/config/vite.config.ts",
    "typecheck": "tsc --noEmit",
    "server": "tsc -p server.tsconfig.json && node dist/server.js",
    "server:dev": "tsx watch server.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
